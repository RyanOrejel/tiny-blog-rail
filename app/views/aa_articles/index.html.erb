<p id="notice"><%= notice %></p>

<h1>Aa Articles</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aa_articles.each do |aa_article| %>
      <tr>
        <td><%= aa_article.title %></td>
        <td><%= aa_article.body %></td>
        <td><%= link_to 'Show', aa_article %></td>
        <td><%= link_to 'Edit', edit_aa_article_path(aa_article) %></td>
        <td><%= link_to 'Destroy', aa_article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aa Article', new_aa_article_path %>
